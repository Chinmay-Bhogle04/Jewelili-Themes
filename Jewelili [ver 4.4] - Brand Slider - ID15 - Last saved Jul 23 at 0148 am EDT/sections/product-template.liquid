<div class="product-template__container page-width"
id="ProductSection-{{ section.id }}"
data-section-id="{{ section.id }}"
data-section-type="product"
data-enable-history-state="true"
data-ajax-enabled="{{ settings.enable_ajax }}"
>
{% comment %}
Get first variant, or deep linked one
{% endcomment %}
{%- assign current_variant = product.selected_or_first_available_variant -%}
{% assign featured_media = current_variant.featured_media | default: product.featured_media %}
{% capture mediaList %}
{% for variant in product.variants %}
{{ variant.featured_media.id }}{{ variant.title }}{% if forloop.last == false %}`~`{% endif %}
{% endfor %}
{% endcapture %}

{%- assign product_image_zoom_size = '1024x1024' -%}
{%- assign product_image_scale = '2' -%}
{%- assign enable_image_zoom = section.settings.enable_image_zoom -%}
{%- assign compare_at_price = current_variant.compare_at_price -%}
{%- assign price = current_variant.price -%}

{% case section.settings.media_size %}
{% when 'small' %}
{%- assign product_media_width = 'medium-up--one-third' -%}
{%- assign product_description_width = 'medium-up--two-thirds' -%}
{%- assign height = 345 -%}
{% when 'medium' %}
{%- assign product_media_width = 'medium-up--one-half' -%}
{%- assign product_description_width = 'medium-up--one-half' -%}
{%- assign height = 530 -%}
{% when 'large' %}
{%- assign product_media_width = 'medium-up--two-thirds' -%}
{%- assign product_description_width = 'medium-up--one-third' -%}
{%- assign height = 720 -%}
{% when 'full' %}
{%- assign product_media_width = '' -%}
{%- assign product_description_width = '' -%}
{%- assign height = 1090 -%}
{%- assign enable_image_zoom = false -%}
{% endcase %}

{% assign bespokering = false %}
{% for tag in product.tags %}
{% if tag contains 'bespoke' %}
{% assign bespoke = tag %}
{% endif %}
{% if tag contains 'custom' %}
{% assign custom_tag = tag %}
{% endif %}
{% if tag  contains 'gmst' %}
{% assign NoEngraving = tag %}
{% endif %}
{% if tag  contains 'bespokering' %}
{% assign bespokering = true %}
{% endif %}
{% endfor %}

{% assign hasVariantImage = false %}
{%- if product.tags contains 'Image with Variant' or product.tags contains 'Image With Variant' -%}
{% assign hasVariantImage = true %}
{% endif %}

<div class="grid product-single{% if section.settings.enable_payment_button %} product-single--{{ section.settings.media_size }}-media{% endif %}">
  <div class="grid__item product-single__media-group {{ product_media_width }}{% if section.settings.media_size == 'full' %} product-single__media-group--full{% endif %}" data-product-single-media-group>
    <div class="main-image-wrapper"  data-tag="{% if bespoke %}{{bespoke}}{% endif %}{% if hasVariantImage and bespoke == blank %}variant_thumbnails{% endif %}">
      <div class="percentage-off">
        {% if custom_tag %}
        <p class="customized">Personalized</p>
        {% elsif bespoke %}
        <p class="customized">Customize</p>
        {% elsif product.compare_at_price > product.price %}
        {% capture percentageOff %}{{ product.compare_at_price | minus: product.price | times: 100.0 | divided_by: product.compare_at_price | money_without_currency | times: 100 | remove: '.0'}}{% endcapture %}
        {% assign number = percentageOff | plus:0 %}
        {% include 'svg-icons', svgId: number %}
        {% endif %}
      </div>
      <div class="mm__product-gallery">
        {%- for media in product.media -%}
        {% assign active_media = false %}
        {% if featured_media == media %}
        {% assign active_media = true %}
        {% endif %}
        {% assign mediaId = media.id %}
        {% if mediaList contains mediaId -%}
        {% assign colorName1 = mediaList | split: mediaId | last %}
        {% endif %}
        {% include 'media', media: media, colorName1:colorName1, featured_media: featured_media, height: height, enable_image_zoom: enable_image_zoom, image_zoom_size: product_image_zoom_size, image_scale: product_image_scale %}
        {%- endfor -%}
      </div>
      {%- assign featured_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media -%}
      <div class="share-wrapper">
        <span class="share-button" title="Share">
          <svg height="40px" viewBox="-21 0 512 512" width="40px" xmlns="http://www.w3.org/2000/svg"><path d="m453.332031 85.332031c0 38.292969-31.039062 69.335938-69.332031 69.335938s-69.332031-31.042969-69.332031-69.335938c0-38.289062 31.039062-69.332031 69.332031-69.332031s69.332031 31.042969 69.332031 69.332031zm0 0"/><path d="m384 170.667969c-47.0625 0-85.332031-38.273438-85.332031-85.335938 0-47.058593 38.269531-85.332031 85.332031-85.332031s85.332031 38.273438 85.332031 85.332031c0 47.0625-38.269531 85.335938-85.332031 85.335938zm0-138.667969c-29.417969 0-53.332031 23.9375-53.332031 53.332031 0 29.398438 23.914062 53.335938 53.332031 53.335938s53.332031-23.9375 53.332031-53.335938c0-29.394531-23.914062-53.332031-53.332031-53.332031zm0 0"/><path d="m453.332031 426.667969c0 38.289062-31.039062 69.332031-69.332031 69.332031s-69.332031-31.042969-69.332031-69.332031c0-38.292969 31.039062-69.335938 69.332031-69.335938s69.332031 31.042969 69.332031 69.335938zm0 0"/><path d="m384 512c-47.0625 0-85.332031-38.273438-85.332031-85.332031 0-47.0625 38.269531-85.335938 85.332031-85.335938s85.332031 38.273438 85.332031 85.335938c0 47.058593-38.269531 85.332031-85.332031 85.332031zm0-138.667969c-29.417969 0-53.332031 23.9375-53.332031 53.335938 0 29.394531 23.914062 53.332031 53.332031 53.332031s53.332031-23.9375 53.332031-53.332031c0-29.398438-23.914062-53.335938-53.332031-53.335938zm0 0"/><path d="m154.667969 256c0 38.292969-31.042969 69.332031-69.335938 69.332031-38.289062 0-69.332031-31.039062-69.332031-69.332031s31.042969-69.332031 69.332031-69.332031c38.292969 0 69.335938 31.039062 69.335938 69.332031zm0 0"/><path d="m85.332031 341.332031c-47.058593 0-85.332031-38.269531-85.332031-85.332031s38.273438-85.332031 85.332031-85.332031c47.0625 0 85.335938 38.269531 85.335938 85.332031s-38.273438 85.332031-85.335938 85.332031zm0-138.664062c-29.417969 0-53.332031 23.933593-53.332031 53.332031s23.914062 53.332031 53.332031 53.332031c29.421875 0 53.335938-23.933593 53.335938-53.332031s-23.914063-53.332031-53.335938-53.332031zm0 0"/><path d="m135.703125 245.761719c-7.425781 0-14.636719-3.863281-18.5625-10.773438-5.824219-10.21875-2.238281-23.253906 7.980469-29.101562l197.949218-112.851563c10.21875-5.867187 23.253907-2.28125 29.101563 7.976563 5.824219 10.21875 2.238281 23.253906-7.980469 29.101562l-197.953125 112.851563c-3.328125 1.898437-6.953125 2.796875-10.535156 2.796875zm0 0"/><path d="m333.632812 421.761719c-3.585937 0-7.210937-.898438-10.539062-2.796875l-197.953125-112.851563c-10.21875-5.824219-13.800781-18.859375-7.976563-29.101562 5.800782-10.238281 18.855469-13.84375 29.097657-7.976563l197.953125 112.851563c10.21875 5.824219 13.800781 18.859375 7.976562 29.101562-3.945312 6.910157-11.15625 10.773438-18.558594 10.773438zm0 0"/></svg>
        </span>
        <ul class="social-sharing hide">
          <li>
            <a target="_blank" href="//www.facebook.com/sharer.php?u={{ shop.url | append: share_permalink }}{{ product.url }}" class="btn btn--small btn--share share-facebook">
              {% include 'icon-facebook' %}
            </a>
          </li>
          <li>
            <a target="_blank" href="//twitter.com/share?text={{ product.title }}&amp;url={{ shop.url | append: share_permalink }}{{ product.url }}" class="btn btn--small btn--share share-twitter">
              {% include 'icon-twitter' %}
            </a>
          </li>
          <li>
            <a target="_blank" rel="noopener" 
            href="https://wa.me/?text={{ product.title }} {{ shop.url | append: share_permalink }}{{ product.url }}" class="btn btn--small btn--share share-pinterest">
            {% include 'icon-pinterest' %}
          </a>
        </li>
        
        <li>
          <a target="_blank" href="mailto:?Subject=Enchanted Disney Fine Jewelry&amp;Body=I%20saw%20this%20and%20thought%20of%20you!%20 {{ shop.url | append: share_permalink }}{{ product.url }}" class="btn btn--small btn--share share-facebook">
            {% include 'icon-mail' %}
          </a>
        </li>
      </ul>
    </div>
  </div>
  
  {% assign first_3d_model = product.media | where: "media_type", "model" | first %}
  
  {%- if first_3d_model -%}
  <button aria-label="{{ 'products.product.view_in_space_label' | t }}" class="product-single__view-in-space" data-shopify-xr data-shopify-model3d-id="{{ first_3d_model.id }}"
  data-shopify-title="{{ product.title | escape }}" data-shopify-xr-hidden >
  {% include 'icon-3d-badge-full-color' %}<span class='product-single__view-in-space-text'>{{ 'products.product.view_in_space' | t }}</span>
</button>
{%- endif -%}


{% if product.media.size > 1 %}
{% if product.media.size > 4 %}
{%- assign enable_thumbnail_slides = true -%}
{% endif %}
<div class="thumbnails-wrapper{% if enable_thumbnail_slides == true %} thumbnails-slider--active{% endif %}">
  <noscript>
    {% capture product_image_size %}{{ height }}x{% endcapture %}
    <img src="{{ featured_media | img_url: product_image_size, scale: product_image_scale }}" alt="{{ featured_media.alt }}" id="FeaturedMedia-{{ section.id }}" class="product-featured-media" style="max-width: {{ height }}px;">
  </noscript>
  {% comment %}
  {% if enable_thumbnail_slides == true %}
  <button type="button" class="btn btn--link medium-up--hide thumbnails-slider__btn thumbnails-slider__prev thumbnails-slider__prev--{{ section.id }}">
    {% include 'icon-chevron-left' %}
    <span class="icon__fallback-text">{{ 'sections.slideshow.previous_slide' | t }}</span>
  </button>
  {% endif %}
  {% endcomment %}
  <ul class="product-single__thumbnails product-single__thumbnails-{{ section.id }} {% if bespoke %}bespoke_thumbnail{% endif %}">
    {% for media in product.media %}
    
    {% assign active_media = false %}
    {% if featured_media == media %}
    {% assign active_media = true %}
    {% endif %}
    {% assign mediaId = media.id %}
    {% if mediaList contains mediaId -%}
    {% assign colorName1 = mediaList | split: mediaId | last %}
    {% endif %}
    
    <li class="product-single__thumbnails-item product-single__thumbnails-item--{{ section.settings.media_size }} {{ colorName1 | split:"`~`" | first | handle | replace: "sterling-silver","14k-white-gold" | replace: "10k-white-gold","14k-white-gold" | replace: "10k-yellow-gold","14k-yellow-gold" | replace: "10k-rose-gold","14k-rose-gold" }} js">
      {% comment %}
      <a href="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"
      class="text-link product-single__thumbnail product-single__thumbnail--{{ section.id }}"
      data-thumbnail-id="{{ section.id }}-{{ media.id }}"
      {% if enable_image_zoom %}data-zoom="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>
      {% endcomment %}
      {%- capture thumbnailAlt -%}
      {%- if media.media_type == 'video' or media.media_type == 'external_video' -%}
      {{ 'sections.featured_product.video_thumbnail_alt' | t: imageAlt: media.alt | escape }}
      {%- elsif media.media_type == 'model' -%}
      {{ 'sections.featured_product.model_thumbnail_alt' | t: imageAlt: media.alt | escape }}
      {%- else -%}
      {{ 'sections.featured_product.gallery_thumbnail_alt' | t: imageAlt: media.alt | escape }}
      {%- endif -%}
      {%- endcapture -%}
      
      <img class="product-single__thumbnail-image" src="{{ media.preview_image | img_url: '110x110', scale: 2 }}" data-thumbnail-id="{{ section.id }}-{{ media.id }}" alt="{{ thumbnailAlt }}">
      {%- if media.media_type == 'video' or media.media_type =='external_video' -%}
      <div class="product-single__thumbnail-badge">
        {% include 'icon-video-badge-full-color' %}
      </div>
      {%- endif -%}
      {%- if media.media_type == 'model' -%}
      <div class="product-single__thumbnail-badge">
        {% include 'icon-3d-badge-full-color' %}
      </div>
      {%- endif -%}
      {% comment %}
    </a>
    {% endcomment %}
  </li>
  {% endfor %}
</ul>
{% comment %}
{% if enable_thumbnail_slides == true %}
<button type="button" class="btn btn--link medium-up--hide thumbnails-slider__btn thumbnails-slider__next thumbnails-slider__next--{{ section.id }}">
  {% include 'icon-chevron-right' %}
  <span class="icon__fallback-text">{{ 'sections.slideshow.next_slide' | t }}</span>
</button>
{% endif %}
{% endcomment %}
</div>
{% endif %}
</div>


<div class="grid__item product-single__detail-group {{ product_description_width }}">
  <div class="product-single__meta">
    
    <div class="product__vendor"><span>{{ product.vendor }}</span></div>
    
    <h1 class="product-single__title">{{ product.title }}</h1>	  
    
    <p class="product-single__sku">SKU: <span class="variant-sku">{{ current_variant.sku }}</span></p>
    
    <div class="medium--up-hide">{% include 'ri-rating-product' with product %}</div>
    
    <div class="product__price">
      {% include 'product-price', variant: current_variant, show_vendor: section.settings.show_vendor %}
    </div>
    
    <div class="free-shipping-text coupan-text hide">
      <p>
        <span>
          <svg id="Capa_1" enable-background="new 0 0 512.006 512.006" viewBox="0 0 512.006 512.006" xmlns="http://www.w3.org/2000/svg"><path id="XMLID_1468_" d="m502.335 77.815 6.741-6.741c3.905-3.905 3.905-10.237 0-14.143-3.905-3.904-10.237-3.904-14.143 0l-6.741 6.741-6.737-6.737c-1.875-1.875-4.419-2.929-7.071-2.929h-10.999l-51.077-51.077c-1.876-1.875-4.419-2.929-7.071-2.929h-369.222c-19.858 0-36.014 16.155-36.014 36.014v187.401c0 19.858 16.156 36.014 36.014 36.014h87.12l-58.585 58.586c-6.802 6.802-10.548 15.846-10.548 25.466 0 9.619 3.746 18.663 10.548 25.466l132.512 132.512c6.802 6.802 15.846 10.548 25.466 10.548s18.664-3.746 25.466-10.548l99.247-99.246c3.906-3.905 3.906-10.237 0-14.143-3.905-3.903-10.237-3.904-14.142 0l-99.248 99.246c-3.024 3.025-7.046 4.69-11.323 4.69s-8.299-1.665-11.323-4.69l-132.513-132.512c-3.024-3.024-4.69-7.046-4.69-11.323 0-4.278 1.666-8.299 4.69-11.323l258.15-258.15h133.401l3.808 3.808-23.124 23.124c-3.917-1.878-8.3-2.931-12.926-2.931-16.542 0-30 13.458-30 30s13.458 30 30 30 30-13.458 30-30c0-4.626-1.054-9.01-2.932-12.926l23.124-23.124 3.808 3.808v133.4l-95.264 95.265c-3.905 3.905-3.905 10.237 0 14.143 3.905 3.904 10.237 3.904 14.143 0l98.193-98.193c1.875-1.876 2.929-4.419 2.929-7.071v-141.688c0-2.652-1.054-5.195-2.929-7.071zm-322.755 101.62c-5.516 0-10.003-4.487-10.003-10.003 0-5.517 4.487-10.004 10.003-10.004s10.003 4.487 10.003 10.004c0 5.515-4.487 10.003-10.003 10.003zm153.12-125.428c-2.652 0-5.196 1.054-7.071 2.929l-116.294 116.293c.158-1.245.249-2.511.249-3.798 0-16.544-13.459-30.004-30.003-30.004-16.543 0-30.003 13.46-30.003 30.004s13.459 30.003 30.003 30.003c1.287 0 2.553-.091 3.798-.249l-40.243 40.243h-107.121c-8.83 0-16.014-7.184-16.014-16.014v-187.4c0-8.83 7.184-16.014 16.014-16.014h365.079l34.007 34.007zm105.301 84c-5.514 0-10-4.486-10-10s4.486-10 10-10c2.282 0 4.382.777 6.067 2.069.265.346.547.684.863 1s.654.598 1 .863c1.292 1.685 2.069 3.785 2.069 6.067.001 5.515-4.485 10.001-9.999 10.001z"/><path id="XMLID_1527_" d="m332.