            font-weight: 600;
            display: block;
            text-transform: uppercase;
            padding: 5px 10px 5px;
            border: 1px solid #B14A6B;           
            margin-bottom: 10px;
            font-size: 12px;
            clear: both;
            float: right;

            @media(max-width: 768px) {
              padding: 5px 10px;
              line-height: 14px;
              height: 25px;
              font-size: 11px;
            }
          }
          .sold-out {
            background: #B14A6B;
            opacity: 1;      
            color: #fff;
            display: none;
          }
          .sale {
            background: rgba(255, 255, 255, 0.92);
          }
        }
        &.has-hover-img:hover .primary-image{
          opacity: 0;
          visibility: hidden;
        }
        &.has-hover-img:hover .images-two{
          opacity: 1;
          visibility: visible;
        }
        .images-two {
          position: absolute;
          width: 100%;
          left: 0;
          z-index: 1;
          visibility: hidden;
          opacity: 0;
          top: 50%;
          transform: translate(0, -50%);
        }
      }
      .mm-product-info {
        padding: 0 0 15px;
        text-align: center;
        
        .mm-product-title {
          line-height: 21px;
          display: inline-block;
          width: 100%;
          color: $text-color;
          font-size: 16px;
          overflow: hidden;
          display: -webkit-box;
          -webkit-line-clamp: 2;
          -webkit-box-orient: vertical;
          margin-bottom:5px;
          // margin-top: 15px;
          margin-top: 0px;
          font-family: 'Playfair Display', serif;
          font-weight: 600;
          
          @media(max-width: 768px) {
            font-size: 14px;
          }
        }
        .mm-product-vendor {
          line-height: 1.4em;
          // padding: 0 0 5px;
          padding: 8px 0 2px;
          margin: 0;
          color: $text-color;
          font-size: 14px;
          font-family: "Overpass",sans-serif;
        }
        .mm-product-price {
          display: -webkit-box; 
          display: -moz-box;
          display: flex;
          flex-wrap: wrap;
          align-items: center;
          justify-content: center;
          margin-bottom: 0;
          .mm-price,
          .mm-sale-price {
            font-weight: 600; 
            font-size: 18px;
            color: $text-color;
          }
          .mm-original-price {
            margin-left: 10px;
            text-decoration: line-through;
            font-size: 14px;
            color: #748090;
            
            @media(max-width: 767px) {
              margin: 0;
            }
          }
        }
      }
      
      .btn {
        padding: 8px 12px;
        background: #646A76;
        border-radius: 4px;
        font-family: 'Lato', sans-serif;
        letter-spacing: 1.65px;
        font-size: 14px;
        font-weight: 800;
        color: #FFFFFF;
        display: block;
        width: 100%;
        
        svg {
          margin-left: 8px;
          vertical-align: -1px;
        }
      }
    }
    .mm-pagination {
      margin: 30px 0;
      text-align: center;
      @media (max-width: 768px) {
        margin: 0;
      }
      
      a, .current {
        padding: 7px 15px;
        display: inline-block;
        margin-bottom: 10px;
        color: $text-color;
        
        @media(max-width: 767px) {
          padding: 5px 10px;
        }
        
        &:hover {
          background: $primary-color;
          color: #ffffff;
          border-color: $primary-color;
        }
      }
      .current {
        color: #3e4448;
        border-color: $primary-color;
      }
