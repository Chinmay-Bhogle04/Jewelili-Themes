.mm-sort-content {
  margin-left: auto;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  
  .mm-filter-heading {
    h4 {
      @media(max-width: 767px) {
        margin: 0;
      }
    }
  }
  
  select {
    padding: 0;
    outline: none;
    font-size: 16px;
    border: none;
    min-height: 40px;
    color: #B14A6B;
    background-image: url('https://cdn.shopify.com/s/files/1/0432/1243/9712/files/Capture.jpg?v=1595443508');
    background-position: right 45%;
    background-repeat: no-repeat;
    @media(max-width: 768px) {
      width: 100%;
      font-size: 18px;
      color: #292d36;
      font-weight: 400;
      border: 1px solid rgba(217,217,217,0.4);
      border-radius: 0px;
    }
  }
}

.mm-collection {
  .mm-collection-title {
    margin-bottom: 20px;
    text-align: center;
    
    h2 {
      margin: 0;
      color: $primary-color;
    }
    @media(max-width: 767px) {
      margin-bottom: 10px;
    }
  }
  .mm-collection-description {
    width: 100%;
    max-width: 700px;
    text-align: center;
    margin: 0 auto 30px;
    color: $text-color;
    
    @media(max-width: 767px) {
      margin: 0 auto 10px;
    }
  }
  .mm-collection-breadcrumb {
    margin: 0 -.5em 30px;
    
    @media(max-width: 767px) {
      margin: 0 auto 10px;
      display:none;
    }  
    nav {
      padding: 0 4%;
      margin: 0;
      position: relative;
      
      &:before {
        width: 3.5%;
        content: "";
        border-top: 1px solid #748090;
        position: absolute;
        left: 0;
        top: 49%;
      }
      
      span[aria-hidden="true"] {
        width: 10px;
        height: 10px;
        display: inline-block;
        position: relative;
        
        &:before {
          width: 100%;
          content: "";
          border-top: 1px solid #748090;
          position: absolute;
          left: 0;
          top: 49%;
        }
      }
    }
  }
