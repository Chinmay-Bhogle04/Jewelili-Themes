      .mm-close {
        display: none;
        text-transform: uppercase;
        font-size: 14px;
        cursor: pointer;
        
        @media(max-width: 767px) {
          display: block;
        }
      }
    }
    @media(max-width: 1024px) {
      padding-right: 15px;
    }
    @media(max-width: 767px) {
      padding-right: 0;
    }
