@import url('https://fonts.googleapis.com/css2?family=Overpass:wght@300;400;600;800&display=swap');

$overpass-font: 'Overpass', sans-serif;

$primary-color: {{ settings.mm_primary_color }};
$border-color: {{ settings.mm_secondary_color }};
$text-color: #292D36;

.main-content{
  @media (max-width:767px){
    padding-top:0;
  }
  h4{
    font-family:'Lato', sans-serif;
    font-weight:500;
  }
}

.mm-collection-page-width{
  max-width:1280px;
  padding:0 27px;
  margin:0 auto;
  @media (max-width:767px){
    padding:0 15px;
  }
}

.count-product-index-2{
  @media (max-width:767px){
    display:none;
  }
}

