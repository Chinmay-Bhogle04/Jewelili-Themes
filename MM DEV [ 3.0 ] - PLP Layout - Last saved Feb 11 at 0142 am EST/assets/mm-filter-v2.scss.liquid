        width: 100%;
        border: 1px solid $border-color;
        padding-right: 40px;
        outline: none;
        
        &::placeholder {font-size: 14px;}
      }
      svg {
        position: absolute;
        top: 20px;
        right: 10px;
        opacity: 0.2;
        max-width: 20px;
      }
    }
    .filter-content {
      position: absolute;
      top: 100%;
      @media (max-width: 767px) {
        position: static;
      }
    }
    .filter-content,
    .filter-content-lthree {
      margin: 0;
      padding: 10px 15px 8px 15px;
      list-style: none;
      min-width: 200px;
      max-height: 250px;
      overflow-x: hidden;
      overflow-y: auto;
      display: none;
      border: 1px solid $border-color;
      background: #FFF;
      @media (max-width: 767px) {
        padding: 0px;
        border: none;
      }
      
      .filter-item {
        padding: 2px 0;
        cursor: pointer;
        line-height: 1.5em;
        text-align: left;
        
        .filter-item-checkbox {
          height: 16px;
          width: 16px;
          display: inline-block;
          border: 1px solid $border-color;
          vertical-align: middle;
          margin-right: 5px;
          position: relative;
          top: -2px;
          
          &.color {
            height: unset;
            width: unset;
            
            span {
              border: 2px solid #ffffff;
              display: block;
              width: 20px;
              height: 20px;
            }
          }
        }
        .filter-item-name {
          font-size: 15px;
          vertical-align: middle;
          text-transform: capitalize;
          color: $text-color;
          @media (max-width: 768px) {
            font-size: 14px;
          }
        }
        &.filter-selected {
          .filter-item-checkbox:not(.color) {
            &:after {
              content: '';
              width: 9px;
              height: 6px;
              position: absolute;
              top: 3px;
              left: 2px;
              border: 2px solid #2a2a2a;
              border-top: none;
              border-right: none;
              background: 0 0;
              transform: rotate(-50deg);
              box-sizing: border-box;
            }
          }
          .filter-item-checkbox.color {
            border-color: $primary-color;
          }
        }
        &.color-swatch-enabled {
          display: inline-block;
          width: auto;
          
          .filter-item-swatch.color {
            display: block;
            height: 35px;
            width: 35px;
            border-radius: 50%;
            border: 2px solid #f3f3f3;
            
            &:hover {border-color: $primary-color;}
          }
          &.filter-selected {
            .filter-item-swatch.color {
              border-color: $primary-color;
            }
          }
        }
        &.size-swatch-enabled {
          display: inline-block;
          margin-right: 2px;
          margin-bottom: 10px;
          border: 1px solid #e3e3e3;
          padding: 5px 10px;
          text-align: center;
          min-width: 45px;
          width: auto;
          
          &:hover {border-color: $primary-color;}
          
          .filter-item-name {
            text-transform: uppercase;
          }         
          &.filter-selected {
            border-color: $primary-color;
            background: $primary-color;
            color: #ffffff;
            
            .filter-item-name {
              color: #ffffff;
            }
          }
        }
      }
    }
    .filter-content-ltwo {
      padding: 10px 0 0 20px;      
    } 
    .filter-content-lthree {
      padding-top: 10px;
    }
  }
  
  .product-container {
    .no-products {
      text-align: center;
    }
    .mm-product-card {
      position: relative;
      margin-bottom: 25px;
      border-radius: 6px;
      
      .mm-product-image {
        position: relative;
        height: 250px;
        border: 1px solid #E6E6E6;
        border-radius: 6px;
        overflow:hidden;
        .custom_tag{
          position: absolute;
          top: 2px;
          left: 3px;
          z-index: 3;
          .customized {
            font-family: 'Playfair Display', serif;
            font-size: 13px;
            font-weight: 500;
            background: #6e5ca8;
            color: #fff;
            width: 110px;
            padding: 3px 10px 3px 22px;
            position: relative;
            z-index: 0;
            &::before{
              content: '';
              position: absolute;
              top: 3px;
              left: -10px;
              width: 19px;
              height: 19px;
              background-color: #fff;
              transform: rotate(134deg);
              clip-path: polygon(0% 0%, 100% 0%, 0% 100%, 0% 0%);
            }
            &::after{
              content: '';
              position: absolute;
              top: 4px;
              right: -9px;
              width: 18px;
              height: 18px;
              background-color: #6e5ca8;
              transform: rotate(135deg);
              z-index: -1;
            }
          }
        } 

        .percentage-off {
          position: absolute;
          z-index: 5;
          left: 5px;
          background: #fff;
        }
        @media(max-width: 1024px) {
          height: 200px;
        }

        .mm-product-link {
          display: block;
          height: 100%;
        }
        img {
          height: 100%;
          width: 100%;
          object-fit: contain;
          object-position: center;
          padding: 0 20px;
          @media(max-width: 767px) {
            padding: 0 10px;
          }
